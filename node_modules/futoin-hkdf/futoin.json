{
  "name": "futoin-hkdf",
  "version": "1.3.1",
  "vcs": "git",
  "rms": "npm",
  "rmsRepo": "ignored",
  "plugins": {
    "release": "futoin.cid.misc.releasetool"
  },
  "actions": {
    "test": [
      "@cid tool envexec node -- sh -c \"test $NODE_ENV != production\"",
      "@cte grunt test"
    ],
    "upgrade-deps": [
      "@cid tool update",
      "@cte yarn upgrade --latest"
    ]
  }
}
