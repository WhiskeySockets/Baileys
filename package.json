{
  "name": "whatsapp-cloudflare-workers",
  "version": "6.7.16-worker-1.0.0",
  "description": "A version of baileys that works on cloudflare workers",
  "keywords": [
    "whatsapp",
    "automation"
  ],
  "homepage": "https://github.com/rafaelsg-01/whatsapp-cloudflare-workers",
  "repository": {
    "url": "git@github.com:rafaelsg-01/whatsapp-cloudflare-workers.git"
  },
  "license": "MIT",
  "author": "Rafael Silva",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "WAProto/*",
    "WASignalGroup/*.js",
    "engine-requirements.js",
    "node_modules_custom/*"
  ],
  "scripts": {
    "build:all": "tsc && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"dev": "wrangler dev",
    "example": "node --inspect -r ts-node/register Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "prepack": "tsc",
    "prepare": "tsc",
    "preinstall": "node ./engine-requirements.js",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "dev": "wrangler dev",
    "@adiwajshing/keyed-db": "^0.2.4",
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^9.1.3",
    "@protobufjs/aspromise": "^1.1.2",
    "@protobufjs/base64": "^1.1.2",
    "@protobufjs/codegen": "^2.0.4",
    "@protobufjs/eventemitter": "^1.1.0",
    "@protobufjs/fetch": "^1.1.0",
    "@protobufjs/float": "^1.0.2",
    "@protobufjs/inquire": "^1.1.0",
    "@protobufjs/path": "^1.1.2",
    "@protobufjs/pool": "^1.1.0",
    "@protobufjs/utf8": "^1.1.0",
    "cache-manager": "^5.7.6",
    "curve25519-js": "^0.0.4",
    "link-preview-js": "^3.0.14",
    "lodash": "^4.17.21",
    "long": "^5.3.1",
    "pako": "^2.1.0",
    "pino": "^9.6",
    "qrcode-terminal": "^0.12.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250303.0",
    "@types/node": "^16.18.126",
    "axios": "^1.8.2",
    "eslint": "^8.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.8.1",
    "typedoc": "^0.24.7",
    "typescript": "^4.6.4",
    "wrangler": "^3.114.0"
  },
  "peerDependenciesMeta": {
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  }
}
